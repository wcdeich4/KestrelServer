{
  "Logging": {
      "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Kestrel": {
      "Endpoints": {
      "Http": {
        "Url": "http://localhost:5003"
      }
    },
      "Limits": {
        "MaxConcurrentConnections": 100,
        "MaxConcurrentUpgradedConnections": 100
      },
      "DisableStringReuse": true
    },
    "ConnectionStrings": {
      "YourConnectionString": "YourConnectionString = ............."
    },
    "FileFolder": "wwwroot",
    "UseHttpStrictTransportSecurityProtocol": false,
    "CustomErrorsMode": "Off",
"AllowedHosts": "*"
}